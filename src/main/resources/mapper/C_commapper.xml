<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.Creview_Usermapper">
    <insert id="insertC_comm" parameterType="com.pojo.C_com">
        insert into c_com(c_name, c_time, c_text, c_class) values (#{c_name},#{c_time},#{c_text},#{c_class})
    </insert>
</mapper>