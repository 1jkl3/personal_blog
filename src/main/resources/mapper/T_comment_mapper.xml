<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.Review_Usermapper">
    <insert id="SetReview" parameterType="com.pojo.T_comment">
            insert into t_comment(t_name,t_img,t_text,t_time) values (#{t_name},#{t_img},#{t_text},#{t_time})
    </insert>
</mapper>